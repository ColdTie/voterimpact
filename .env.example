REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here
REACT_APP_API_URL=http://localhost:3001

# Feature Flags for Dynamic Data (default: false for safety)
REACT_APP_USE_REAL_POLITICIANS=false
REACT_APP_USE_REAL_LEGISLATION=false

# API Keys for Political Data
# Get your free API key at https://api.data.gov or use DEMO_KEY for testing
REACT_APP_CONGRESS_API_KEY=DEMO_KEY
REACT_APP_OPENSTATES_API_KEY=your_openstates_api_key_here

# DEMO_KEY limits: 30 requests/hour, 50 requests/day per IP
# Production limits: 1,000 requests/hour with your own API key